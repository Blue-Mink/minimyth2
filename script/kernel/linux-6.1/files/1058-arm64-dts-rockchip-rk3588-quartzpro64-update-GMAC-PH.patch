From c615059ab3a177b60c1832a98f881ce383fdebbf Mon Sep 17 00:00:00 2001
From: Andrew Powers-Holmes <aholmes@omnom.net>
Date: Wed, 19 Oct 2022 01:49:35 +1100
Subject: [PATCH 58/63] arm64: dts: rockchip: rk3588-quartzpro64: update
 GMAC+PHY to current DT schema

---
 arch/arm64/boot/dts/rockchip/rk3588-quartzpro64.dts | 10 +++++-----
 1 file changed, 5 insertions(+), 5 deletions(-)

diff --git a/arch/arm64/boot/dts/rockchip/rk3588-quartzpro64.dts b/arch/arm64/boot/dts/rockchip/rk3588-quartzpro64.dts
index 8cec3a90506..7f1b0e05ba2 100644
--- a/arch/arm64/boot/dts/rockchip/rk3588-quartzpro64.dts
+++ b/arch/arm64/boot/dts/rockchip/rk3588-quartzpro64.dts
@@ -187,14 +187,10 @@ &gpu {
 };
 
 &gmac0 {
+	label = "RJ45";
 	phy-mode = "rgmii-rxid";
 	clock_in_out = "output";
 
-	snps,reset-gpio = <&gpio4 RK_PB3 GPIO_ACTIVE_LOW>;
-	snps,reset-active-low;
-	/* Reset time is 20ms, 100ms for rtl8211f */
-	snps,reset-delays-us = <0 20000 100000>;
-
 	pinctrl-names = "default";
 	pinctrl-0 = <&gmac0_miim
 		     &gmac0_tx_bus2
@@ -215,6 +211,10 @@ phy1: ethernet-phy@1 {
 		compatible = "ethernet-phy-ieee802.3-c22";
 		reg = <1>;
 		max-speed = <1000>;
+		/* Reset time is 20ms, 100ms for rtl8211f */
+		reset-gpios = <&gpio4 RK_PB3 GPIO_ACTIVE_LOW>;
+		reset-assert-us = <20000>;
+		reset-deassert-us = <100000>;
 	};
 };
 
