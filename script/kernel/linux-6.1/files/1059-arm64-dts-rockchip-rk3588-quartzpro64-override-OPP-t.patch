From: Andrew Powers-Holmes <aholmes@omnom.net>
Date: Wed, 2 Nov 2022 22:55:26 +1100
Subject: [PATCH 59/63] arm64: dts: rockchip: rk3588-quartzpro64: override OPP
 tables a little

---
 .../boot/dts/rockchip/rk3588-quartzpro64.dts  | 76 +++++++++++++++++++
 1 file changed, 76 insertions(+)

diff --git a/arch/arm64/boot/dts/rockchip/rk3588-quartzpro64.dts b/arch/arm64/boot/dts/rockchip/rk3588-quartzpro64.dts
index 7f1b0e05ba2..802676f8c61 100644
--- a/arch/arm64/boot/dts/rockchip/rk3588-quartzpro64.dts
+++ b/arch/arm64/boot/dts/rockchip/rk3588-quartzpro64.dts
@@ -176,6 +176,82 @@ &cpu_b2 {
 	mem-supply = <&vdd_cpu_big1_mem_s0>;
 };
 
+&cluster0_opp_table {
+	/delete-node/ opp-408000000;
+	/delete-node/ opp-600000000;
+	/delete-node/ opp-816000000;
+};
+
+&cluster1_opp_table {
+	/delete-node/ opp-408000000;
+	/delete-node/ opp-600000000;
+	/delete-node/ opp-816000000;
+
+	opp-2256000000 {
+		opp-supported-hw = <0xfd 0x13>;
+		opp-hz = /bits/ 64 <2256000000>;
+		opp-microvolt = <1000000 1000000 1000000>,
+				<1000000 1000000 1000000>;
+		clock-latency-ns = <40000>;
+	};
+	opp-2304000000 {
+		opp-supported-hw = <0xfd 0x24>;
+		opp-hz = /bits/ 64 <2304000000>;
+		opp-microvolt = <1000000 1000000 1000000>,
+				<1000000 1000000 1000000>;
+		clock-latency-ns = <40000>;
+	};
+	opp-2352000000 {
+		opp-supported-hw = <0xfd 0x48>;
+		opp-hz = /bits/ 64 <2352000000>;
+		opp-microvolt = <1000000 1000000 1000000>,
+				<1000000 1000000 1000000>;
+		clock-latency-ns = <40000>;
+	};
+	opp-2400000000 {
+		opp-supported-hw = <0xfd 0x80>;
+		opp-hz = /bits/ 64 <2400000000>;
+		opp-microvolt = <1000000 1000000 1000000>,
+				<1000000 1000000 1000000>;
+		clock-latency-ns = <40000>;
+	};
+};
+
+&cluster2_opp_table {
+	/delete-node/ opp-408000000;
+	/delete-node/ opp-600000000;
+	/delete-node/ opp-816000000;
+
+	opp-2256000000 {
+		opp-supported-hw = <0xfd 0x13>;
+		opp-hz = /bits/ 64 <2256000000>;
+		opp-microvolt = <1000000 1000000 1000000>,
+				<1000000 1000000 1000000>;
+		clock-latency-ns = <40000>;
+	};
+	opp-2304000000 {
+		opp-supported-hw = <0xfd 0x24>;
+		opp-hz = /bits/ 64 <2304000000>;
+		opp-microvolt = <1000000 1000000 1000000>,
+				<1000000 1000000 1000000>;
+		clock-latency-ns = <40000>;
+	};
+	opp-2352000000 {
+		opp-supported-hw = <0xfd 0x48>;
+		opp-hz = /bits/ 64 <2352000000>;
+		opp-microvolt = <1000000 1000000 1000000>,
+				<1000000 1000000 1000000>;
+		clock-latency-ns = <40000>;
+	};
+	opp-2400000000 {
+		opp-supported-hw = <0xfd 0x80>;
+		opp-hz = /bits/ 64 <2400000000>;
+		opp-microvolt = <1000000 1000000 1000000>,
+				<1000000 1000000 1000000>;
+		clock-latency-ns = <40000>;
+	};
+};
+
 &tsadc {
 	status = "okay";
 };
