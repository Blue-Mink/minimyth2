diff -Naur linux-5.2.2-old/arch/arm64/boot/dts/allwinner/sun50i-h6-eachlink-h6mini.dts linux-5.2.2-new/arch/arm64/boot/dts/allwinner/sun50i-h6-eachlink-h6mini.dts
