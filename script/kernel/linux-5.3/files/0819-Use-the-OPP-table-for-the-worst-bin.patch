From e4f0fef2be0bf46f51b337fcf17279ada5567d7e Mon Sep 17 00:00:00 2001
From: Ondrej Jirman <megous@megous.com>
Date: Wed, 24 Apr 2019 11:17:54 +0200
Subject: [PATCH 19/39] Use the OPP table for the worst bin

---
 arch/arm64/boot/dts/allwinner/sun50i-h6.dtsi | 20 +++++++++++++-------
 1 file changed, 13 insertions(+), 7 deletions(-)

diff --git a/arch/arm64/boot/dts/allwinner/sun50i-h6.dtsi b/arch/arm64/boot/dts/allwinner/sun50i-h6.dtsi
index 4b2afbc..5fd30c6 100644
--- a/arch/arm64/boot/dts/allwinner/sun50i-h6.dtsi
+++ b/arch/arm64/boot/dts/allwinner/sun50i-h6.dtsi
@@ -73,43 +73,49 @@
 
 		opp@480000000 {
 			opp-hz = /bits/ 64 <480000000>;
-			opp-microvolt = <800000 800000 880000>;
+			opp-microvolt = <880000 880000 880000>;
 			clock-latency-ns = <244144>; /* 8 32k periods */
 		};
 
 		opp@720000000 {
 			opp-hz = /bits/ 64 <720000000>;
-			opp-microvolt = <800000 800000 880000>;
+			opp-microvolt = <880000 880000 880000>;
 			clock-latency-ns = <244144>; /* 8 32k periods */
 		};
 
 		opp@816000000 {
 			opp-hz = /bits/ 64 <816000000>;
-			opp-microvolt = <800000 800000 880000>;
+			opp-microvolt = <880000 880000 880000>;
 			clock-latency-ns = <244144>; /* 8 32k periods */
 		};
 
 		opp@888000000 {
 			opp-hz = /bits/ 64 <888000000>;
-			opp-microvolt = <800000 800000 940000>;
+			opp-microvolt = <880000 880000 880000>;
 			clock-latency-ns = <244144>; /* 8 32k periods */
 		};
 
 		opp@1080000000 {
 			opp-hz = /bits/ 64 <1080000000>;
-			opp-microvolt = <840000 840000 1060000>;
+			opp-microvolt = <940000 940000 940000>;
 			clock-latency-ns = <244144>; /* 8 32k periods */
 		};
 
 		opp@1320000000 {
 			opp-hz = /bits/ 64 <1320000000>;
-			opp-microvolt = <900000 900000 1160000>;
+			opp-microvolt = <1000000 1000000 1000000>;
 			clock-latency-ns = <244144>; /* 8 32k periods */
 		};
 
 		opp@1488000000 {
 			opp-hz = /bits/ 64 <1488000000>;
-			opp-microvolt = <960000 960000 1160000>;
+			opp-microvolt = <1060000 1060000 1060000>;
+			clock-latency-ns = <244144>; /* 8 32k periods */
+		};
+
+		opp@1800000000 {
+			opp-hz = /bits/ 64 <1800000000>;
+			opp-microvolt = <1160000 1160000 1160000>;
 			clock-latency-ns = <244144>; /* 8 32k periods */
 		};
 	};
