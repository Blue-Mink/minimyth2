From d054ed59255d0e1bb236962bc1cc93ddab13b224 Mon Sep 17 00:00:00 2001
From: Neil Armstrong <narmstrong@baylibre.com>
Date: Tue, 26 Feb 2019 11:21:41 +0100
Subject: [PATCH 151/178] WIP: arm64: dts: meson-g12a: add missing
 drive-strength for sdcard, sdio and hdmi ddc

---
 arch/arm64/boot/dts/amlogic/meson-g12a.dtsi | 6 +++++-
 1 file changed, 5 insertions(+), 1 deletion(-)

diff --git a/arch/arm64/boot/dts/amlogic/meson-g12a.dtsi b/arch/arm64/boot/dts/amlogic/meson-g12a.dtsi
index 5915108..5fd7041 100644
--- a/arch/arm64/boot/dts/amlogic/meson-g12a.dtsi
+++ b/arch/arm64/boot/dts/amlogic/meson-g12a.dtsi
@@ -212,6 +212,7 @@
 								 "hdmitx_sck";
 							function = "hdmitx";
 							bias-disable;
+							drive-strength = <4000>;
 						};
 					};
 
@@ -302,6 +303,7 @@
 								 "sdcard_clk_c";
 							function = "sdcard";
 							bias-pull-up;
+							drive-strength = <4000>;
 						};
 					};
 
@@ -323,6 +325,7 @@
 								 "sdcard_clk_z";
 							function = "sdcard";
 							bias-pull-up;
+							drive-strength = <4000>;
 						};
 					};
 
@@ -343,7 +346,8 @@
 								 "sdio_cmd",
 								 "sdio_clk";
 							function = "sdio";
-							bias-disable;
+							bias-pull-up;
+							drive-strength = <4000>;
 						};
 					};
 
