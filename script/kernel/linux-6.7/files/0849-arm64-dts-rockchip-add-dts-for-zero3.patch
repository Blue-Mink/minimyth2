diff --speed-large-files --no-dereference --minimal -Naur linux-6.7-rc8/arch/arm64/boot/dts/rockchip/rk3566-radxa-zero-3w.dts linux-6.7-rc8/arch/arm64/boot/dts/rockchip/rk3566-radxa-zero-3w.dts
