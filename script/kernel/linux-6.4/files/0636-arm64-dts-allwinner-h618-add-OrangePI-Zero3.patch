diff --speed-large-files --no-dereference --minimal -Naur linux-6.4.7/arch/arm64/boot/dts/allwinner/sun50i-h618-orangepi-zero3.dts linux-6.4.7/arch/arm64/boot/dts/allwinner/sun50i-h618-orangepi-zero3.dts
