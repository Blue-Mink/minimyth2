From aa9066fe1efcaca694f8ed325a63921ac8398b67 Mon Sep 17 00:00:00 2001
From: Paul Harrison <pharrison@mythtv.org>
Date: Sun, 6 Jul 2014 10:59:39 +0100
Subject: [PATCH] MythNetVision: NULL check item in NetSearch::customEvent()

Fixes Coverity ID 1224479 Dereference after null check
---
 mythplugins/mythnetvision/mythnetvision/netsearch.cpp |    6 +++++-
 1 file changed, 5 insertions(+), 1 deletion(-)

diff --git a/mythplugins/mythnetvision/mythnetvision/netsearch.cpp b/mythplugins/mythnetvision/mythnetvision/netsearch.cpp
index 2376b3f..ff75ddb 100644
--- a/mythplugins/mythnetvision/mythnetvision/netsearch.cpp
+++ b/mythplugins/mythnetvision/mythnetvision/netsearch.cpp
@@ -510,7 +511,10 @@ void NetSearch::customEvent(QEvent *event)
 
         MythUIButtonListItem *item = m_searchResultList->GetItemAt(pos);
 
-        if (item && item->GetText() == title)
+        if (!item)
+            return;
+
+        if (item->GetText() == title)
             item->SetImage(file);
 
         if (m_searchResultList->GetItemCurrent() == item)
