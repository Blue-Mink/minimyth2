From 6a75882075ad0b092f8d50b94b9bc92ead4cd067 Mon Sep 17 00:00:00 2001
From: Jean-Yves Avenard <jyavenard@mythtv.org>
Date: Thu, 15 May 2014 10:49:04 +1000
Subject: [PATCH 4/4] Fix 5.1 audio being configured as 7.1

Thanks to StuartM for spotting it
---
 mythtv/libs/libmyth/audio/audiooutputca.cpp |    2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/mythtv/libs/libmyth/audio/audiooutputca.cpp b/mythtv/libs/libmyth/audio/audiooutputca.cpp
index 2d0cc00..fabd1af 100644
--- a/mythtv/libs/libmyth/audio/audiooutputca.cpp
+++ b/mythtv/libs/libmyth/audio/audiooutputca.cpp
@@ -69,7 +69,6 @@ QString StreamDescriptionToString(AudioStreamBasicDescription desc)
             .arg(fourCC)
             .arg((desc.mFormatFlags & kAudioFormatFlagIsBigEndian) ? "BE" : "LE")
             .arg((UInt32)desc.mSampleRate);
-
             break;
         default:
             str = QString("[%1]").arg(fourCC);
@@ -1251,6 +1250,7 @@ int CoreAudioData::OpenAnalog()
         case 6:
             //  3F2-LFE        L   R   C    LFE  LS   RS
             new_layout.mChannelLayoutTag = kAudioChannelLayoutTag_AudioUnit_5_1;
+            break;
         case 8:
             // We need
             // 3F4-LFE        L   R   C    LFE  Rls  Rrs  LS   RS
