---
 mythtv/libs/libmythbase/logging.cpp |   10 +++++-----
 1 file changed, 5 insertions(+), 5 deletions(-)

diff --git a/mythtv/libs/libmythbase/logging.cpp b/mythtv/libs/libmythbase/logging.cpp
index 8345a28..4c5b26d 100644
--- a/mythtv/libs/libmythbase/logging.cpp
+++ b/mythtv/libs/libmythbase/logging.cpp
@@ -175,22 +175,22 @@ LoggingItem::LoggingItem(const char *_file, const char *_function,
 LoggingItem::~LoggingItem()
 {
     if (m_file)
-        free((void *)m_file);
+        free(const_cast<char*>(m_file));
 
     if (m_function)
-        free((void *)m_function);
+        free(const_cast<char*>(m_function));
 
     if (m_threadName)
         free(m_threadName);
 
     if (m_appName)
-        free((void *)m_appName);
+        free(const_cast<char*>(m_appName));
 
     if (m_table)
-        free((void *)m_table);
+        free(const_cast<char*>(m_table));
 
     if (m_logFile)
-        free((void *)m_logFile);
+        free(const_cast<char*>(m_logFile));
 }
 
 QByteArray LoggingItem::toByteArray(void)
