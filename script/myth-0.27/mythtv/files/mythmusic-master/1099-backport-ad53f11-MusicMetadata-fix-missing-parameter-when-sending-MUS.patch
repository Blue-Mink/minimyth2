From ad53f11b6291e87caa04e0650e91779902a751df Mon Sep 17 00:00:00 2001
From: Paul Harrison <pharrison@mythtv.org>
Date: Sun, 16 Mar 2014 21:26:37 +0000
Subject: [PATCH 01/11] MusicMetadata: fix missing parameter when sending
 MUSIC_FIND_ALBUMART command

---
 mythtv/libs/libmythmetadata/musicmetadata.cpp |    3 ++-
 1 file changed, 2 insertions(+), 1 deletion(-)

diff --git a/mythtv/libs/libmythmetadata/musicmetadata.cpp b/mythtv/libs/libmythmetadata/musicmetadata.cpp
index 5f28f77..fc669af 100644
--- a/mythtv/libs/libmythmetadata/musicmetadata.cpp
+++ b/mythtv/libs/libmythmetadata/musicmetadata.cpp
@@ -1793,7 +1793,8 @@ void AlbumArtImages::scanForImages()
     QStringList strList;
     strList << "MUSIC_FIND_ALBUMART"
             << m_parent->Hostname()
-            << QString::number(m_parent->ID());
+            << QString::number(m_parent->ID())
+            << "1";
 
     AlbumArtScannerThread *scanThread = new AlbumArtScannerThread(strList);
     scanThread->start();
